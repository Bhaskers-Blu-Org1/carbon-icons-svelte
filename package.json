{
  "name": "carbon-icons-svelte",
  "version": "10.8.2",
  "license": "Apache-2.0",
  "description": "Svelte components for icons in digital and software products using the Carbon Design System",
  "author": "Eric Liu (https://github.com/metonym)",
  "svelte": "lib/index.js",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc && node dist",
    "test": "jest --coverage",
    "test:tdd": "jest --watch",
    "prepublishOnly": "yarn build"
  },
  "devDependencies": {
    "@carbon/icon-helpers": "10.5.1",
    "@carbon/icons": "10.8.0",
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^8.3.4",
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "^24.9.1",
    "fs-extra": "^8.1.0",
    "husky": "^3.1.0",
    "jest": "^24.9.0",
    "lint-staged": "^10.0.3",
    "prettier": "^1.19.1",
    "pretty-quick": "^2.0.1",
    "ts-jest": "^24.2.0",
    "tslint": "^6.0.0",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^3.7.5"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged && pretty-quick --staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.{ts}": [
      "tslint --fix -p .",
      "git add"
    ]
  },
  "prettier": {
    "printWidth": 100,
    "tabWidth": 2,
    "semi": true,
    "singleQuote": true
  },
  "jest": {
    "preset": "ts-jest"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/ibm/carbon-icons-svelte.git"
  },
  "homepage": "https://github.com/ibm/carbon-icons-svelte",
  "keywords": [
    "carbon",
    "carbon icons",
    "svelte"
  ],
  "files": [
    "lib"
  ],
  "bugs": "https://github.com/ibm/carbon-icons-svelte/issues",
  "sideEffects": false
}
